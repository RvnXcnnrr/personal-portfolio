# EmailJS Configuration
# Get these values from https://dashboard.emailjs.com/

# Your EmailJS Service ID (from Email Services section)
VITE_EMAILJS_SERVICE_ID=service_your_service_id

# Your EmailJS Template ID (from Email Templates section)
VITE_EMAILJS_TEMPLATE_ID=template_your_template_id

# Your EmailJS Public Key (from Account section > API Keys)
VITE_EMAILJS_PUBLIC_KEY=your_public_key

# Optional: Testimonial template ID
VITE_EMAILJS_TESTIMONIAL_TEMPLATE_ID=template_testimonial_id
